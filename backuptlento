  CONSTRAINT `company_id_id_5912bfe7_fk_base_company_id` FOREIGN KEY (`company_id_id`) REFERENCES `base_company` (`id`),
  CONSTRAINT `created_by_id_1c73f9ba_fk_auth_user_id` FOREIGN KEY (`created_by_id`) REFERENCES `auth_user` (`id`),
  CONSTRAINT `modified_by_id_947a87fd_fk_auth_user_id` FOREIGN KEY (`modified_by_id`) REFERENCES `auth_user` (`id`),
