--
-- Table structure for table `base_historicalrotatingshiftassign`
--

DROP TABLE IF EXISTS `base_historicalrotatingshiftassign`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `base_historicalrotatingshiftassign` (
  `id` bigint(20) NOT NULL,
  `created_at` datetime(6) DEFAULT NULL,
  `is_active` tinyint(1) NOT NULL,
  `history_title` varchar(20) DEFAULT NULL,
  `history_description` longtext DEFAULT NULL,
  `history_highlight` tinyint(1) DEFAULT NULL,
  `start_date` date NOT NULL,
  `next_change_date` date DEFAULT NULL,
  `based_on` varchar(10) NOT NULL,
  `rotate_after_day` int(11) DEFAULT NULL,
  `rotate_every_weekend` varchar(10) DEFAULT NULL,
  `rotate_every` varchar(10) DEFAULT NULL,
  `additional_data` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_bin DEFAULT NULL CHECK (json_valid(`additional_data`)),
  `history_id` int(11) NOT NULL AUTO_INCREMENT,
  `history_date` datetime(6) NOT NULL,
  `history_change_reason` varchar(100) DEFAULT NULL,
  `history_type` varchar(1) NOT NULL,
  `created_by_id` int(11) DEFAULT NULL,
  `current_shift_id` bigint(20) DEFAULT NULL,
  `employee_id_id` bigint(20) DEFAULT NULL,
  `history_relation_id` bigint(20) NOT NULL,
  `history_user_id` int(11) DEFAULT NULL,
  `modified_by_id` int(11) DEFAULT NULL,
  `next_shift_id` bigint(20) DEFAULT NULL,
  `rotating_shift_id_id` bigint(20) DEFAULT NULL,
