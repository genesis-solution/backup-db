  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `badge_id` varchar(50) DEFAULT NULL,
  `employee_first_name` varchar(200) NOT NULL,
  `employee_last_name` varchar(200) DEFAULT NULL,
  `employee_profile` varchar(100) DEFAULT NULL,
