/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `payroll_allowance` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `created_at` datetime(6) DEFAULT NULL,
  `is_active` tinyint(1) NOT NULL,
  `title` varchar(191) NOT NULL,
  `one_time_date` date DEFAULT NULL,
  `include_active_employees` tinyint(1) NOT NULL,
  `is_taxable` tinyint(1) NOT NULL,
  `is_condition_based` tinyint(1) NOT NULL,
  `field` varchar(191) DEFAULT NULL,
  `condition` varchar(191) DEFAULT NULL,
  `value` varchar(191) DEFAULT NULL,
  `is_fixed` tinyint(1) NOT NULL,
  `amount` double DEFAULT NULL,
  `based_on` varchar(191) DEFAULT NULL,
  `rate` double DEFAULT NULL,
  `per_attendance_fixed_amount` double DEFAULT NULL,
  `per_children_fixed_amount` double DEFAULT NULL,
  `shift_per_attendance_amount` double DEFAULT NULL,
  `amount_per_one_hr` double DEFAULT NULL,
  `work_type_per_attendance_amount` double DEFAULT NULL,
  `has_max_limit` tinyint(1) NOT NULL,
  `maximum_amount` double DEFAULT NULL,
  `maximum_unit` varchar(20) DEFAULT NULL,
  `if_choice` varchar(10) NOT NULL,
  `if_condition` varchar(10) NOT NULL,
  `if_amount` double NOT NULL,
  `start_range` double DEFAULT NULL,
  `end_range` double DEFAULT NULL,
  `only_show_under_employee` tinyint(1) NOT NULL,
  `is_loan` tinyint(1) NOT NULL,
  `company_id_id` bigint(20) DEFAULT NULL,
  `created_by_id` int(11) DEFAULT NULL,
  `modified_by_id` int(11) DEFAULT NULL,
  `shift_id_id` bigint(20) DEFAULT NULL,
  `work_type_id_id` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `title_is_taxable_is_cond_6fe89b5e_uniq` (`title`,`is_taxable`,`is_condition_based`,`field`,`condition`,`value`,`is_fixed`,`amount`,`based_on`,`rate`,`per_attendance_fixed_amount`,`shift_id_id`,`shift_per_attendance_amount`,`amount_per_one_hr`,`work_type_id_id`,`work_type_per_attendance_amount`) USING HASH,
  KEY `shift_id_id_2a990c10_fk_base_employeeshift_id` (`shift_id_id`),
  KEY `work_type_id_id_da9a0d44_fk_base_worktype_id` (`work_type_id_id`),
  KEY `company_id_id_5912bfe7_fk_base_company_id` (`company_id_id`),
  KEY `created_by_id_1c73f9ba_fk_auth_user_id` (`created_by_id`),
