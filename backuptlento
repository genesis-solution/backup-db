--
-- Table structure for table `employee_bonuspoint`
--

DROP TABLE IF EXISTS `employee_bonuspoint`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `employee_bonuspoint` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `created_at` datetime(6) DEFAULT NULL,
  `is_active` tinyint(1) NOT NULL,
  `points` int(11) NOT NULL,
  `encashment_condition` varchar(100) DEFAULT NULL,
  `redeeming_points` int(11) DEFAULT NULL,
  `reason` longtext DEFAULT NULL,
  `created_by_id` int(11) DEFAULT NULL,
  `employee_id_id` bigint(20) DEFAULT NULL,
  `modified_by_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `employee_id_id` (`employee_id_id`),
  KEY `employee_bonuspoint_modified_by_id_3041354b_fk_auth_user_id` (`modified_by_id`),
  KEY `employee_bonuspoint_created_by_id_2ce413ab_fk_auth_user_id` (`created_by_id`),
