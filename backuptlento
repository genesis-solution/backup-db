  `phone` varchar(15) NOT NULL,
  `address` longtext DEFAULT NULL,
  `country` varchar(30) DEFAULT NULL,
  `state` varchar(30) DEFAULT NULL,
  `city` varchar(30) DEFAULT NULL,
  `zip` varchar(20) DEFAULT NULL,
  `dob` date DEFAULT NULL,
  `gender` varchar(10) DEFAULT NULL,
  `qualification` varchar(50) DEFAULT NULL,
  `experience` int(11) DEFAULT NULL,
  `marital_status` varchar(50) DEFAULT NULL,
  `children` int(11) DEFAULT NULL,
  `emergency_contact` varchar(15) DEFAULT NULL,
  `emergency_contact_name` varchar(20) DEFAULT NULL,
  `emergency_contact_relation` varchar(20) DEFAULT NULL,
  `is_active` tinyint(1) NOT NULL,
  `additional_info` longtext CHARACTER SET utf8mb4 COLLATE utf8mb4_bin DEFAULT NULL CHECK (json_valid(`additional_info`)),
  `is_from_onboarding` tinyint(1) DEFAULT NULL,
  `is_directly_converted` tinyint(1) DEFAULT NULL,
  `employee_user_id_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `email` (`email`),
  UNIQUE KEY `employee_user_id_id` (`employee_user_id_id`),
  UNIQUE KEY `employee_employee_employee_first_name_empl_b193a07f_uniq` (`employee_first_name`,`employee_last_name`,`email`),
  CONSTRAINT `employee_employee_employee_user_id_id_4c6da5a5_fk_auth_user_id` FOREIGN KEY (`employee_user_id_id`) REFERENCES `auth_user` (`id`)
